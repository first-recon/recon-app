    "react-native-svg": "6.0.1-rc.1",
    "react-native-svg-uri": "1.2.3",
    "react-navigation": "1.0.0-beta.21"
  },
  "devDependencies": {
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "4.0.0",
    "jest": "21.2.1",
